{
  "name": "quikapi/framework",
  "description": "QuikAPI - a lightweight, fast PHP microframework for building APIs quickly.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "vlucas/phpdotenv": "^5.6",
    "symfony/console": "^6.4",
    "illuminate/database": "^10.0",
    "illuminate/events": "^10.0",
    "illuminate/validation": "^10.0",
    "illuminate/translation": "^10.0",
    "firebase/php-jwt": "^6.10"
  },
  "autoload": {
    "psr-4": {
      "QuikAPI\\": "./"
    }
  },
  "bin": [
    "bin/quikapi"
  ],
  "support": {
    "issues": "https://github.com/QuikAPI/framework/issues",
    "source": "https://github.com/QuikAPI/framework"
  }
}
